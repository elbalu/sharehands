<div class="well" id="listPost">
	<div class="row">
	{#data.posts}
	
	<div class="col-sm-6 col-md-3 post-{type}">
		<div class="thumbnail">
    		<img src="/img/{imgUrl}">
    		<div class="caption">
    			<h3>{title}</h3>
    			<p>
      				<strong>{email}</strong>&#160;{type}&#160;{title}
      			</p>
      			<button type="submit" class="btn btn-primary btn-lg btn-block">{buttonAction}</button>
  			</div>
  		</div>
  	</div>
	{/data.posts}
</div>
</div>